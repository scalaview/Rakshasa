<header id="myCarousel">
  <ul class="bxslider" style="margin: 0px;">
    {{#each banners}}
      <li>
        <a href="{{#if url }}{{ url }}{{else}}#{{/if}}">
          <img class="image-response" src="{{ image }}"/>
        </a>
      </li>
    {{/each}}
  </ul>
</header>

<script>
$(document).ready(function(){
  $('.bxslider').bxSlider({
    auto: true,
    speed: 2000
  });
});
</script>

<style type="text/css">
.bx-wrapper {
    margin: 0px;
}
</style>